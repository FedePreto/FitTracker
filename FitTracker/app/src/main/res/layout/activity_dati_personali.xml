<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".autenticazione.DatiPersonali" android:background="@color/green_bg">

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="35dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:background="@drawable/btn_bg" android:id="@+id/linearLayout">
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/rounded_rectangle"
                android:id="@+id/imageView22" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/rounded_rectangle"
                android:id="@+id/imageView23" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/rounded_rectangle"
                android:id="@+id/imageView24" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/rounded_rectangle_vuoto"
                android:id="@+id/imageView25" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/rounded_rectangle_vuoto"
                android:id="@+id/imageView26" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/rounded_rectangle_vuoto"
                android:id="@+id/imageView27" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/rounded_rectangle_vuoto"
                android:id="@+id/imageView28" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/rounded_rectangle_vuoto"
                android:id="@+id/imageView29" android:layout_weight="1"/>
    </LinearLayout>
    <TextView
            android:text="Dati Personali"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"
            android:textSize="55sp" android:textStyle="bold|italic"
            android:textAllCaps="false" android:textAlignment="center" android:textColor="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout3" app:layout_constraintVertical_bias="0.405"/>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="335dp"
            android:layout_height="57dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.493" android:id="@+id/linearLayout3"
            android:background="@drawable/btn_bg" app:layout_constraintBottom_toTopOf="@+id/tE_name"
            android:layout_marginBottom="56dp">
        <TextView
                android:text="Data di nascita:"
                android:layout_width="56dp"
                android:layout_marginLeft="10dp"
                android:gravity="center"
                android:layout_height="match_parent" android:id="@+id/textView5" android:layout_weight="1"
                android:textStyle="bold" android:textSize="18sp" android:textAlignment="center"/>
        <EditText
                android:layout_width="228dp"
                android:layout_marginRight="15dp"
                android:layout_height="match_parent"
                android:inputType="date"
                android:ems="10"
                android:id="@+id/eT_dataNascita" android:layout_weight="1" android:textSize="25sp"
                android:textAlignment="center" android:hint="GG-MM-AAAA"/>
    </LinearLayout>
    <TextView
            android:layout_width="295dp"
            android:layout_height="40dp" android:id="@+id/tE_name"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/tE_surname"
            android:layout_marginBottom="44dp" android:background="@drawable/btn_bg" android:textSize="25sp"
            android:textAlignment="center" android:hint="Nome"/>
    <TextView
            android:layout_width="295dp"
            android:layout_height="40dp" android:id="@+id/tE_surname"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/bt_Avanti"
            android:layout_marginBottom="84dp" android:textSize="25sp" android:hint="Cognome"
            android:background="@drawable/btn_bg" android:textAlignment="center"/>
    <Button
            android:text="AVANTI"
            android:layout_width="225dp"
            android:layout_height="65dp" android:id="@+id/bt_Avanti"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="68dp" android:background="@drawable/btn_bg" android:textSize="25sp"
            android:textStyle="bold"/>
</androidx.constraintlayout.widget.ConstraintLayout>