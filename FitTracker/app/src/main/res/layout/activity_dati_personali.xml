                android:textColor="#FFFFFF" android:textAlignment="center"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="323dp"
            android:layout_height="54dp"
            android:id="@+id/linearLayout4"
            tools:ignore="MissingConstraints" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="15dp">
