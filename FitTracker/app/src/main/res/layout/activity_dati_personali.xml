                android:text="Quadricipite"
                android:layout_width="165dp"
                android:layout_height="match_parent" android:id="@+id/textView12"
                android:textStyle="bold" android:textAlignment="center"
                android:textSize="25sp"
                android:gravity="center"/>
        <EditText

                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="numberSigned"
                android:ems="10"
                android:id="@+id/gamba" android:layout_weight="1" android:textSize="25sp"
                android:textColor="#FFFFFF" android:textAlignment="center"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="323dp"
            android:layout_height="54dp"
            android:id="@+id/linearLayout11"
            tools:ignore="MissingConstraints" app:layout_constraintTop_toBottomOf="@+id/linearLayout9"
