<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:context=".autenticazione.ObbiettivoActivity"
>

    <Button
            android:text="AVANTI"
            android:layout_width="225dp"
            android:layout_height="65dp" android:id="@+id/bt_Avanti"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.497"
            android:layout_marginBottom="50dp" android:textStyle="bold" android:background="@drawable/btn_bg_2"
            android:textSize="25sp"/>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="223dp"
            android:layout_height="44dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/linearLayout2"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="50dp">
        <ImageView
                android:layout_width="31dp"
                android:layout_height="match_parent" app:srcCompat="@drawable/color_logo_no_background_logo"
                android:id="@+id/imageView4" android:layout_weight="1"/>
        <TextView
                android:text="Conosciamoci meglio!"
                android:layout_width="154dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content" android:id="@+id/textView13" android:layout_weight="1"
                android:textStyle="bold" android:textAlignment="center" android:textSize="17sp"
                android:textColor="@color/green_primary_dark"/>
    </LinearLayout>
    <LinearLayout
            android:layout_marginLeft="50dp"
            android:layout_marginRight="50dp"
            android:orientation="horizontal"
            android:layout_width="364dp"
            android:layout_height="59dp"
            android:gravity="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/liniette">
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/linietta" android:id="@+id/imageView10"
                android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/linietta_grigia"
                android:id="@+id/imageView11" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/linietta_grigia"
                android:id="@+id/imageView12" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/linietta_grigia"
                android:id="@+id/imageView13" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/linietta_grigia"
                android:id="@+id/imageView14" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/linietta_grigia"
                android:id="@+id/imageView15" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/linietta_grigia"
                android:id="@+id/imageView16" android:layout_weight="1"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" app:srcCompat="@drawable/linietta_grigia"
                android:id="@+id/imageView71" android:layout_weight="1"/>
    </LinearLayout>
    <androidx.fragment.app.FragmentContainerView
            android:name="androidx.navigation.fragment.NavHostFragment"
            android:layout_width="0dp"
            android:layout_height="0dp" app:navGraph="@navigation/nav_graph" app:defaultNavHost="true"
            android:id="@+id/NavHost" android:layout_marginBottom="1dp"
            app:layout_constraintBottom_toTopOf="@+id/bt_Avanti" android:layout_marginTop="1dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2" app:layout_constraintVertical_bias="0.0"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>